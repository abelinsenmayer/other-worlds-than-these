{
	"schemaVersion": 1,
	"id": "owtt",
	"version": "${version}",
	"name": "Other Worlds Than These",
	"description": "A mod by Abe Linsenmayer that adds dimension-related features to Minecraft.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/owtt/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "abe.owtt.OtherWorldsThanThese",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "abe.owtt.OtherWorldsThanTheseClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "abe.owtt.OtherWorldsThanTheseDataGenerator",
				"adapter": "kotlin"
			}
		],
		"cardinal-components": [
			"abe.owtt.component.OwttComponents"
		]
	},
	"mixins": [
		"owtt.mixins.json",
		{
			"config": "owtt.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.3",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.9.22"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"cardinal-components": [
			"owtt:cosmic_alignment"
		]
	}
}